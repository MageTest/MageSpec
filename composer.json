{
    "name": "magetest/magento-phpspec-extension",
    "description": "Magento PHPSpec extension",
    "require": {
        "phpspec/phpspec": "dev-master",
        "phpspec/prophecy": "dev-master",
        "symfony/dependency-injection": "*",
        "symfony/config": "*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimun-stability": "dev",
    "autoload": {
        "psr-0": {
            "MageTest\\PhpSpec\\MagentoExtension": "src",
            "": [
                "src",
                "vendor/magento/lib",
                "vendor/magento/app"
            ],
            "Mage": "vendor/magento/app/code/core"
        }
    },
    "authors": [
